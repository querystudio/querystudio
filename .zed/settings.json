{
  "file_scan_exclusions": [
    "src/routeTree.gen.ts",
    "src/vite-env.d.ts",
    ".tanstack",
    ".git",
    "tsconfig.node.json",
    ".vscode",
    ".cursor",
    ".output",
    "node_modules"
  ],
  "lsp": {
    "oxlint": {
      "initialization_options": {
        "settings": {
          "configPath": null,
          "run": "onType",
          "disableNestedConfig": false,
          "fixKind": "safe_fix",
          "typeAware": true,
          "unusedDisableDirectives": "deny"
        }
      }
    },
    "oxfmt": {
      "initialization_options": {
        "settings": {
          "configPath": null,
          "run": "onSave"
        }
      }
    }
  }
}
