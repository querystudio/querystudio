{
  "identifier": "desktop-capability",
  "platforms": ["macOS", "windows", "linux"],
  "windows": ["main", "settings"],
  "permissions": [
    "updater:default",
    "process:default",
    "deep-link:default",
    "http:default",
    "os:default",
    "sql:default",
    "opener:default",
    {
      "identifier": "opener:allow-open-url",
      "allow": [
        {
          "url": "https://querystudio.dev"
        }
      ]
    }
  ]
}
